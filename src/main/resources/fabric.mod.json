{
  "schemaVersion": 1,
  "id": "archipelago",
  "version": "${version}",
  "name": "Archipelago",
  "description": "Minecraft client for Archipelago",
  "authors": ["Cootshk"],
  "contact": {},
  "license": "GPL-3.0",
  "icon": "assets/archipelago/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "io.github.cootshk.archipelago.client.ArchipelagoClient"
      }
    ],
    "main": [
      {
        "adapter": "kotlin",
        "value": "io.github.cootshk.archipelago.Archipelago"
      }
    ]
  },
  "mixins": [
    "archipelago.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}"
  }
}
